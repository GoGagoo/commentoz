{
	"compilerOptions": {
		"baseUrl": ".",
		"moduleResolution": "node",
		"paths": {
			"@/*": ["src/*"],
			"@styles/*": ["src/styles/*"]
		},
		"outDir": "./dist/",
		"sourceMap": true,
		"strict": true,
		"module": "CommonJS",
		"target": "es5",
		"jsx": "react",
		"allowJs": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true
	}
}
